
<h1><%= title %></h1>

<div class="container">
	<dl>
		<% categories.sort((a, b) => { let textA = a.name.toUpperCase(); let textB = b.name.toUpperCase(); return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;}); %>
		<% categories.forEach(function(category) { %>
			<dt><a href="<%= category.url %>" class="blue"><%= category.name %></a></dt>
		<% }); %>
	<dl>
</div>